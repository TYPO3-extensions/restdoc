<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>General</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<path>
						<TCEforms>
							<exclude>1</exclude>
							<label>Documentation Root:</label>
							<config>
								<type>input</type>
								<size>48</size>
								<eval>required,trim</eval>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Select the path to the documentation</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard&amp;act=folder</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec,file</blindLinkOptions>
										</params>
										<JSopenParams>height=500,width=400,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</path>
					<mode>
						<TCEforms>
							<exclude>1</exclude>
							<label>Show:</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="1" type="array">
										<numIndex index="0">Body</numIndex>
										<numIndex index="1">BODY</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Table of contents</numIndex>
										<numIndex index="1">TOC</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</mode>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>